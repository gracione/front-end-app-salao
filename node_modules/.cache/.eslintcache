  const { idFuncionario } = useParams();\n\n  const profissoes = BuscarDadosApi('profissao', 'listar');\n\n  const [profissoesAlteradas, setProfissoesAlteradas] = useState(profissao);\n  const profissoesAlteradasAux: any = profissoesAlteradas;\n\n  const [profissoesCadastradas, setProfissoesCadastradas] = useState([]);\n  const profissoesCadastradasAux: any = profissoesCadastradas;\n\n\n  const [quantidadeProfissoes, setQuantidadeProfissoes] = useState(0);\n\n\n  useEffect(() => {\n    api\n      .post(\"/profissao/listar-id-funcionario\", {\n        id: idFuncionario\n      })\n      .then((response) => setProfissao(response.data));\n\n    api\n      .post(\"/funcionarios/listar-id\", {\n        id: idFuncionario\n      })\n      .then((response) => setListagem(response.data));\n\n    api\n      .post(\"/expediente/listar-id\", {\n        id: idFuncionario\n      })\n      .then((response) => setExpediente(response.data));\n\n  }, []);\n\n  function adicionarProfissao(valor: any, indice: any) {\n    profissoesCadastradasAux[indice] = valor;\n    setProfissoesCadastradas(profissoesCadastradasAux);\n  }\n\n  function alterarProfissao(valor: any, indice: any) {\n    profissoesAlteradasAux[indice] = valor;\n    setProfissoesAlteradas(profissoesAlteradasAux);\n  }\n\n  if (!listagem) {\n    return (\n      <Header>\n        <Conteudo>\n          <h2>\n            dados não carregados\n          </h2>\n        </Conteudo>\n      </Header>\n    )\n  }\n  return (\n    <Header>\n      <Conteudo>\n        <h3 >Alterar dados do Funcionário</h3>\n        <label>Nome</label>\n        <input\n          defaultValue={listagem['nome']}\n          onChange={e => setNome(e.target.value)}\n          required\n        />\n\n        <label>Contato(telefone)</label>\n        <input\n          defaultValue={listagem['numero']}\n          onChange={e => setNumero(e.target.value)}\n          required\n        />\n        <option value={0}>Escolha a Profissão</option>\n\n        {profissao.map((element: any) => (\n          <select\n            required\n            onChange={e => alterarProfissao(e.target.value, element.id_funcionario)}\n            defaultValue={element.id}\n          >\n            <option value={'-1'}>Deletar</option>\n            {profissoes.map((element: any) => (\n              <option value={element.id}>{element.profissão}</option>\n            ))}\n          </select>\n        ))}\n\n        <div>\n          {[...Array(quantidadeProfissoes)].map((x, i) =>\n            <select\n              onChange={e => adicionarProfissao(e.target.value, i)}\n              required\n            >\n              <option>Escolha a Profissão</option>\n              {\n                profissoes.map((element) => (\n                  <option value={element.id}>{element.profissão + \" \" + i}</option>\n                ))\n              }\n            </select>\n          )}\n        </div>\n        <AdicionarPrifissao onClick={() => setQuantidadeProfissoes(quantidadeProfissoes + 1)}>\n          Adicionar mais uma profissão\n        </AdicionarPrifissao>\n        <table>\n          <tr>\n            <th colSpan={4}>\n              <Center>\n                Expediente\n              </Center>\n            </th>\n          </tr>\n          <tr>\n            <td>Inicio de expediente</td>\n            <td>Inicio de Horario de Almoço</td>\n            <td>Fim de Horario de Almoço</td>\n            <td>Fim de expediente</td>\n          </tr>\n          <tr>\n            <td>\n              <input\n                defaultValue={expediente['inicio_de_expediente']}\n                onChange={e => setInicioDeExpediente(e.target.value)}\n                type=\"time\"\n              />\n            </td>\n            <td>\n              <input\n                defaultValue={expediente['inicio_horario_de_almoco']}\n                onChange={e => setInicioHorarioAlmoco(e.target.value)}\n                type=\"time\"\n              />\n            </td>\n            <td>\n              <input\n                defaultValue={expediente['fim_horario_de_almoco']}\n                onChange={e => setFimHorarioAlmoco(e.target.value)}\n                type=\"time\"\n              />\n            </td>\n            <td>\n              <input\n                defaultValue={expediente['fim_de_expediente']}\n                onChange={e => setFimExpediente(e.target.value)}\n                type=\"time\"\n              />\n            </td>\n          </tr>\n        </table>\n        <Alterar\n          modulo=\"funcionarios\"\n          dados={{\n            id: idFuncionario, nome, numero, profissoesCadastradas, profissoesAlteradas,\n            expediente: { inicio1: inicioDeExpediente, fim1: inicioHorarioAlmoco, inicio2: fimHorarioAlmoco, fim2: fimExpediente }\n          }}\n        />\n\n      </Conteudo>\n    </Header>\n  );\n}\n","/var/www/html/front-end-app-salao/src/paginas/Funcionarios/inserir.tsx",[],[],"/var/www/html/front-end-app-salao/src/paginas/Profissoes/alterar.tsx",["207","208","209"],[],"import Menu from \"../Menu\";\nimport { Container, Conteudo, Header } from \"../../styles/global\";\nimport Alterar from \"../../util/alterar\";\nimport { useState, useEffect } from \"react\";\nimport api from \"../../services/api\";\nimport { useParams } from \"react-router-dom\";\n\nexport default function AlterarProfissao() {\n  const [nome, setNome] = useState('');\n  const [listagem, setListagem]: any = useState([]);\n  const { idProfissao } = useParams();\n\n  useEffect(() => {\n    api\n      .post(\"/profissao/listar-id\", {\n        id: idProfissao\n      })\n      .then((response) => setListagem(response.data[0]));\n\n  }, []);\n  return (\n    <Header>\n\n      <Conteudo>\n        <h3>Alterar dados da profissão</h3>\n        <small>Nome</small>\n        <input\n          name='nome'\n          placeholder=\"...\"\n          defaultValue={listagem['nome']}\n          onChange={e => setNome(e.target.value)}\n          required\n        />\n        <Alterar\n          modulo=\"profissao\"\n          dados={{ id: idProfissao, nome }}\n        />\n      </Conteudo>\n    </Header>\n\n  );\n}\n","/var/www/html/front-end-app-salao/src/paginas/Autenticacao/Registrar/index.js",["210"],[],"/var/www/html/front-end-app-salao/src/paginas/Autenticacao/Login/index.js",["211","212"],[],"/var/www/html/front-end-app-salao/src/paginas/Tratamentos/inserir.tsx",[],[],"/var/www/html/front-end-app-salao/src/paginas/Expediente/inserir.tsx",["213","214","215","216","217","218"],[],"import { useState } from 'react';\nimport { Conteudo,Header } from \"../../styles/global\";\nimport BuscarDadosApi from \"../../util/util\";\nimport Inserir from \"../../util/inserir\";\n\nexport default function InserirExpediente() {\n  const [inicioExpediente, setInicioExpediente] = useState('');\n  const [inicioAlmoco, setInicioAlmoco] = useState('');\n  const [fimAlmoco, setFimAlmoco] = useState('');\n  const [fimExpediente, setFimExpediente] = useState('');\n  const [idFuncionario, setIdFuncionario] = useState('');\n  const funcionario = BuscarDadosApi('funcionarios', 'listarPorProfissao');\n  const optionFuncionarios: any = [];\n  funcionario.forEach(element => {\n    optionFuncionarios.push(\n      <option value={element.id}>{element.nome}</option>\n    );\n  });\n\n  return (\n    <Header>\n      <Conteudo>\n        <div>\n          <h1>Adicionar Expediente</h1>\n          <select\n            onChange={e => setIdFuncionario(e.target.value)}\n            required\n          >\n            <option >Escolha a Profissão</option>\n            {optionFuncionarios}\n          </select>\n          <input type=\"time\" placeholder=\"Inicio Expediente\" onChange={e => setInicioExpediente(e.target.value)} />\n          <input type=\"time\" placeholder=\"Inicio Almoco\" onChange={e => setInicioAlmoco(e.target.value)} />\n          <input type=\"time\" placeholder=\"Fim Almoco\" onChange={e => setFimAlmoco(e.target.value)} />\n       