\n    e.preventDefault();\n    const data = {\n      nome,\n      numero,\n      id_sexo,\n      email,\n      password,\n      \"password_confirmation\": confirmPassword\n    };\n\n    try {\n      api.post('/registrarCliente', data)\n        .then(async (res) => {\n          if (res.data.token) {\n            const response = await api.post('/registrarCliente', { email, password });\n            localStorage.setItem('token', response.data.access_token);\n            localStorage.setItem('id_usuario', response.data.id_usuario);\n            localStorage.setItem('nome', response.data.nome);\n            window.location.href = \"/home\";\n          } else {\n            if (res.data.nome) {\n              setErroNome(\n                <div className='alerta-erro' >\n                  {res.data.nome[0]}\n                </div>\n              );\n            }\n            if (res.data.numero) {\n              setErroNumero(\n                <div className='alerta-erro' >\n                  {res.data.numero[0]}\n                </div>\n              );\n            }\n            if (res.data.id_sexo) {\n              setErroId_sexo(\n                <div className='alerta-erro' >\n                  {res.data.id_sexo[0]}\n                </div>\n              );\n            }\n            if (res.data.email) {\n              setErroEmail(\n                <div className='alerta-erro' >\n                  {res.data.email[0]}\n                </div>\n              );\n            }\n            if (res.data.password) {\n              setErroPassword(\n                <div className='alerta-erro' >\n                  {res.data.password[0]}\n                </div>\n              );\n            }\n          }\n        });\n    } catch (err) {\n      alert('Erro no cadastro, tente novamente.');\n    }\n  }\n  return (\n    <div className=\"registrar\">\n      <form onSubmit={efetuarRegister}>\n\n        <h4 className='text-primary d-flex justify-content-between' >\n          <Link className=\"bg-primary text-white rounded-circle voltar\" to=\"/login\">\n            <FiArrowLeft size={20} />\n          </Link>\n          <div className='p-2 w-75' >\n            Cadastrar Cliente\n          </div>\n        </h4>\n        <div className='h-10 mb-1 input'>\n          <input\n            placeholder=\"Nome\"\n            value={nome}\n            onChange={e => setNome(e.target.value)}\n            required\n          />\n        </div>\n        {erroNome}\n        <div className='h-10 mb-1 input'>\n          <InputMask\n            className='rounded'\n            mask=\"(99) 9 9999-9999\"\n            placeholder=\"Telefone\"\n            value={numero}\n            onChange={e => setNumero(e.target.value)}\n            required\n          />\n        </div>\n        {erroNumero}\n        <div className='h-10 mb-1 input'>\n          <select\n            className='rounded'\n            onChange={e => setId_Sexo(e.target.value)}\n            required\n          >\n            <option value={0}>Escolha o sexo</option>\n            <option value={1}>Masculino</option>\n            <option value={2}>Feminino</option>\n          </select>\n        </div>\n        {erroId_sexo}\n        <div className='h-10 mb-1 input'>\n          <input\n            className='rounded'\n            placeholder=\"Email\"\n            value={email}\n            type=\"email\"\n            onChange={e => setEmail(e.target.value)}\n            required\n          />\n        </div>\n        {erroEmail}\n        <div className='h-10 mb-1 input'>\n          <input\n            className='rounded'\n            placeholder=\"Senha\"\n            value={password}\n            type=\"password\"\n            onChange={e => setPassword(e.target.value)}\n            required\n          />\n        </div>\n        {erroPassword}\n        <div className='h-10 mb-2g input'>\n          <input\n            className='rounded'\n            placeholder=\"Confirmar Senha\"\n    